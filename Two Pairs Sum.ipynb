   "source": [
    "def two_pairs(n, sum_):\n",
    "    pairs= list()\n",
    "    for i in range(len(n)):\n",
    "        for j in range(i+1, len(n)):\n",
    "            if sum_== n[i]+n[j]:\n",
    "                pairs.append((n[i], n[j]))\n",
    "    return pairs\n",
    "                    \n",
    "two_pairs([1,2,3,4,5,6],9)    "
   ]
